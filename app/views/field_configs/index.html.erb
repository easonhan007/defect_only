<h1>Listing field_configs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Field</th>
    <th>Html type</th>
    <th>Values</th>
    <th>Default</th>
    <th>Project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @field_configs.each do |field_config| %>
  <tr>
    <td><%= field_config.name %></td>
    <td><%= field_config.field %></td>
    <td><%= field_config.html_type %></td>
    <td><%= field_config.values %></td>
    <td><%= field_config.default %></td>
    <td><%= field_config.project_id %></td>
    <td><%= link_to 'Show', field_config %></td>
    <td><%= link_to 'Edit', edit_field_config_path(field_config) %></td>
    <td><%= link_to 'Destroy', field_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Field config', new_field_config_path %>
